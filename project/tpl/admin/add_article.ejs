<% include ./header.ejs%>
<div class="container article">
        <div class="form-group">
            <label for="exampleInputEmail1">标题</label>
            <input id="title" type="text" class="form-control" placeholder="title">
            <label for="exampleInputPassword1">简介</label>
            <textarea id="description" class="form-control" rows="3" placeholder="description"></textarea>
            <label for="exampleInputPassword1">内容</label>
            <textarea id="content" class="form-control" rows="15" placeholder="content"></textarea>
            <label for="exampleInputPassword1">分类</label>
            <select class="form-control" id="select">
                <%for(var i=0;i<cate.length;i++){%>
                <option abc="<%=cate[i]._id%>"><%=cate[i].name%></option>
                <%}%>
            </select>
        </div>
        <button class="btn btn-default" id="article-btn">Submit</button>
</div>
</body>
</html>