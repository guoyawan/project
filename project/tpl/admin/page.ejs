<nav>
    <ul class="pager">
        <li><a href="/admin?page=<%=page-1 %>">上一页</a></li>
        <li>一共 <b><%=count%></b> 条数据，每页 <b><%=limit %></b> 条,当前<b><%=page %></b>/<b><%=pages %></b>页 </li>
        <%if(page===pages){%>
        <li><a href="javascript:;">下一页</a></li>
        <%} else{%>
        <li><a href="/admin?page=<%=page+1 %>">下一页</a></li>
        <%}%>
    </ul>
</nav>
